{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./src",
    "baseUrl": "./src",
    "paths": {
      "@binarySearch/*": ["binarySearch/*"],
      "@bubbleSort/*": ["bubbleSort/*"],
      "@linkedList/*": ["linkedList/*"],
      "@queue/*": ["queue/*"]
    },
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
